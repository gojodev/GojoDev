{
  "hosting": {
    "public": "public/portfolio",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/gojodev/**",
        "destination": "/gojodev/index.html"
      },
      {
        "source": "/caopoints/**",
        "destination": "/caopoints/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
